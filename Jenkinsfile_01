#!groovy

pipeline {
    agent any
    
    def MVHome = tool name: 'maven-1', type: 'maven'

       stages {
        stage("Build") {
            steps {
                sh "${MVHome}/bin/mvn -version"
                sh "${MVHome}/bin/mvn clean install"
            }
        }
    }

    post {
        always {
            cleanWs()
        }
    }
}
